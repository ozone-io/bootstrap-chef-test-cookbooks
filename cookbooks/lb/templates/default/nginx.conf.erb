upstream myapp1 {
<% node.app.upstreams.each_with_index do |host, index| %>
<%= "    server #{host};" %>
<% end %>	
}

server {
    listen 80 default;
    
    access_log <%= node.app.web_dir %>/logs/nginx_access.log;
    error_log <%= node.app.web_dir %>/logs/nginx_error.log;

    location / {
            proxy_pass http://myapp1;
    }
}
